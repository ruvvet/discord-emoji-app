
<style>
  .main-guild-emoji-name {
  font-size: 40px;
  text-align: center;
}
</style>


<div class="main-wrapper">
  <% for (const guild in emojisByGuild){ %>
  <div class="container-gradient main-guild-emoji">
    <div class="main-guild-emoji-name"><%=guild%></div>
    <% if (!emojisByGuild[guild]){ %>
    <div>Upload/Browse the Library to Add Some Emoji</div>
    <%} else {%> <% emojisByGuild[guild].forEach(emoji =>{ %>
      <button
      onclick="selectEmoji(`<%=emoji.id%>`,`<%=emoji.name%>`, `<%=emoji.url%>`)"
      class="btn"
    >
    <img class="emoji" src="<%=emoji.url %>" alt="<%=emoji.name%>" />
</button>
    <%})%> <%}%>
  </div>
  <%}%>
</div>
<div class="my-emojis"><%- include('./partials/useremojis') %></div>
<div class="emoji-details"><%- include('./partials/emojidetails') %></div>

<script>

async function selectEmoji(id, url, name) {
    const response = await fetch('http://localhost:5000/emojidetails', {
      method: 'put',
      body: JSON.stringify({ id, url, name }),
      headers: {
        'Content-Type': 'application/json',
      },
    }).catch(console.log);

    location.reload();
  }



</script>